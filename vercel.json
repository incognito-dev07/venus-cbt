{
  "builds": [
    {
      "src": "*.php",
      "use": "@vercel/php"
    },
    {
      "src": "storage/*.json",
      "use": "@vercel/static"
    },
    {
      "src": "styles/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(css|js|json)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.php"
    }
  ]
}