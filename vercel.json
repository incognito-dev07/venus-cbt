{
  "builds": [
    {
      "src": "interface/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "storage/**/*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/interface/$1"
    }
  ]
}