{
  "functions": {
    "*.php": {
      "runtime": "@vercel/php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/index",
      "dest": "/index.php"
    },
    {
      "src": "/study",
      "dest": "/study.php"
    },
    {
      "src": "/select-test",
      "dest": "/select-test.php"
    },
    {
      "src": "/take-test",
      "dest": "/take-test.php"
    },
    {
      "src": "/submit-test",
      "dest": "/submit-test.php"
    },
    {
      "src": "/profile",
      "dest": "/profile.php"
    },
    {
      "src": "/settings",
      "dest": "/settings.php"
    },
    {
      "src": "/history",
      "dest": "/history.php"
    },
    {
      "src": "/notifications",
      "dest": "/notifications.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/storage/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/icons/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
