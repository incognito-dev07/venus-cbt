{
  "rewrites": [
    {
      "source": "/js/:path",
      "destination": "/js/:path"
    },
    {
      "source": "/css/:path",
      "destination": "/css/:path"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
