{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.php"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/public/css/$1.css"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/public/js/$1.js"
    },
    {
      "src": "/courses.json",
      "dest": "/courses.json"
    },
    {
      "src": "/questions/(.*)",
      "dest": "/questions/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1.php"
    }
  ],
  "env": {
    "APP_NAME": "Venus CBT",
    "APP_ENV": "production"
  }
}
